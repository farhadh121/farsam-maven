<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="true" default-lazy="false">
    <class name="com.radio.entity.hibernate.CategoryEntity"
           table="Category" optimistic-lock="version">

        <id name="categoryID">
            <column name="CategoryID" sql-type="bigint"/>
            <generator class="native"/>
        </id>
        <timestamp name="version" source="db"/>
        
		<property name="name">
			<column name="Name" sql-type="varchar(100)"/>
		</property>

		<set name="songs" table="song_category" cascade="none">
			<key>
				<column name="CategorID" not-null="true"/>
			</key>
			<many-to-many entity-name="com.radio.entity.hibernate.SongEntity">
				<column name="SongID" not-null="true"/>
			</many-to-many>
		</set>

    </class>
</hibernate-mapping>